{
  "name": "iiyatsu",
  "version": "1.0.0",
  "description": "hrfmmymt's iiyatsu",
  "main": "index.js",
  "scripts": {
    "build": "run-p build-index html css",
    "build-index": "tsc ./index.ts ./src/app.ts",
    "build-post-and-sw": "ts-node-dev ./utils/build_post_and_sw",
    "css": "postcss -c postcss.config.js -d ./templates/style/ --ext njk ./src/css/[!base]*",
    "deploy-prd": "vercel --prod",
    "dev": "NODE_ENV=development ts-node-dev index.ts",
    "dev-ssg": "NODE_ENV=development BUILD_SSG=true ts-node-dev index.ts",
    "get-post-info": "ts-node-dev utils/get_post_info.ts",
    "generate-html": "ts-node-dev ./utils/generate_html",
    "html": "run-s build-post-and-sw generate-html",
    "img": "run-s img-opt webp",
    "img-opt": "ts-node-dev ./utils/img-opt.ts",
    "jest": "jest",
    "jest-cov": "jest --coverage",
    "run-set-recent-post-data": "ts-node-dev ./utils/set_recent_post_data.ts",
    "test": "NODE_ENV=test run-s jest",
    "test-cov": "run-s jest-cov",
    "vercel": "vercel",
    "vercel-dev": "vercel dev",
    "webp": "node ./utils/webp.js"
  },
  "keywords": [],
  "author": "hrfmmymt",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.19.2",
    "vercel": "^23.1.2"
  },
  "devDependencies": {
    "@openameba/spindle-syntax-themes": "0.2.0",
    "@tsconfig/node16": "1.0.2",
    "@types/glob": "7.1.4",
    "@types/html-minifier": "4.0.2",
    "@types/jest": "27.4.0",
    "@types/marked": "4.0.1",
    "@types/mz": "2.7.4",
    "@types/node": "16.4.13",
    "@vercel/node": "1.12.1",
    "autoprefixer": "10.3.3",
    "cssnano": "5.0.16",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.24.2",
    "fastify-static": "4.4.1",
    "fs": "0.0.1-security",
    "glob": "7.1.7",
    "highlight.js": "11.2.0",
    "html-minifier": "4.0.0",
    "imagemin": "7.0.1",
    "imagemin-webp": "6.0.0",
    "jest": "27.4.7",
    "jimp": "0.16.1",
    "marked": "4.0.10",
    "mz": "2.7.0",
    "npm-run-all": "4.1.5",
    "nunjucks": "3.2.3",
    "path": "0.12.7",
    "point-of-view": "4.15.1",
    "postcss": "8.3.6",
    "postcss-cli": "8.3.1",
    "postcss-import": "14.0.2",
    "prettier": "2.5.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-prettier": "1.2.0",
    "ts-jest": "27.0.5",
    "ts-node-dev": "1.1.8",
    "typescript": "4.3.5"
  }
}
